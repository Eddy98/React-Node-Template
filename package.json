{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/Eddy98/React-Node-Template#readme",
  "author": "Eddy98",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts"
    ],
    "watch": [
      "src/api"
    ],
    "ext": "js,ts,json"
  },
  "scripts": {
    "build:dev": "NODE_ENV=development npm run build:client",
    "build:client": "webpack",
    "start": "NODE_ENV=production node dist/src/api/server.js --unhandled-rejections=strict",
    "start:dev": "concurrently -p \"[{name}]\" -n static,server -c \"magenta,cyan\" \"npm run start:dev:client\" \"npm run start:dev:server\" --kill-others",
    "start:dev:server": "NODE_ENV=development nodemon --exec ts-node-esm src/api/server.ts",
    "start:dev:client": "NODE_ENV=development webpack serve --open",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
